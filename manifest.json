{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "version": "0.1.0",
  "default_locale": "en",
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_title": "__MSG_extActionTitle__",
    "default_icon": {
      "16": "assets/icons/icon16.png",
      "32": "assets/icons/icon32.png"
    }
  },
  "background": {
    "service_worker": "src/background/index.js",
    "type": "module"
  },
  "icons": {
    "16": "assets/icons/icon16.png",
    "32": "assets/icons/icon32.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "sessions"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "options_page": "src/options/options.html",
  "commands": {
    "go-back-or-close": {
      "suggested_key": {
        "default": "Alt+Left",
        "mac": "Command+Left"
      },
      "description": "__MSG_commandBackOrClose__"
    },
    "go-forward": {
      "suggested_key": {
        "default": "Alt+Right",
        "mac": "Command+Right"
      },
      "description": "__MSG_commandForward__"
    }
  }
}
